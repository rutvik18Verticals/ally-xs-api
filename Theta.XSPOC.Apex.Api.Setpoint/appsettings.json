{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AdvancedLogging": {
    "ApplicationInsights": {
      "EnableAdaptiveSampling": false,
      "EnablePerformanceCounterCollectionModule": false
    }
  },
  "CorsPolicy": {
    "Enabled": true,
    "Origins": "http://localhost:3000|http://localhost:3001"
  }
}
